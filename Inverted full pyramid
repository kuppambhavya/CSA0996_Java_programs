import java.io.*;
class GFG {
	public static void main(String[] args)
	{
		int number = 3;
		int i = number, j;
		while (i > 0) {
			j = 0;
			while (j++ < number - i) {
				System.out.print(" ");
			}
			j = 0;
			while (j++ < (i * 2) - 1) {
				// Print star
				System.out.print("*");
			}
			System.out.println();
			i--;
		}
	}
}
